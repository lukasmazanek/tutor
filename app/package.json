{
  "name": "tutor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "validate": "node ../scripts/validate-data.js",
    "sync-data": "bash ../scripts/sync-data.sh",
    "typecheck": "tsc --noEmit",
